List<ApplicationUser>


<div class="card shadow border-0 my-4">
    <div class="card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-white py-2">User List</h2>
            </div>
        </div>
    </div>
    <div class="card-body p-4">
        <table id="tblData" class="table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>State</th>
                    <th>Company</th>
                    <th>Role</th>
                    <th>lockstatus</th>

                    <th></th>
                </tr>
            </thead>

              <tbody>

            @foreach(var obj in Model)
            {
                  <tr>
                      <td>@obj.Name</td>
                      <td>@obj.Email</td>
                       <td>@obj.State</td>
                      <td>@obj.Company.Name</td>   
                      <td>@obj.Role</td>
                       <td>@obj.lockstatus</td>

                      <td>
                              <div class="w-75 btn-group" role="group">
                                    <a asp-controller="User" asp-action="LockUnlock" asp-route-id="@obj.Id" class="btn btn-secondary mx-2">
                                         <i class="bi bi-pencil-square"></i> Lock-Unlock
                                    </a>
                                 
                              </div>
                     </td>
                  </tr>
            }

        </tbody>
        </table>

    </div>
</div>

@section Scripts{
    <script src="~/js/user.js"></script>
}